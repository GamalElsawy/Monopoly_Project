                <Font size="40.0" />
            </font>
        </Label>
        <Label fx:id="two" layoutX="292.0" layoutY="301.0" onMouseClicked="#twoPlayers" prefHeight="0.0" prefWidth="48.0" style="-fx-background-repeat: no-repeat;" stylesheets="@proStyle.css">
            <font>
                <Font size="37.0" />
            </font>
        </Label>
        <Label fx:id="three" layoutX="420.0" layoutY="305.0" onMouseClicked="#threePlayers" prefHeight="45.0" prefWidth="48.0" style="-fx-background-repeat: no-repeat;" stylesheets="@proStyle.css" />
        <Label fx:id="four" layoutX="561.0" layoutY="305.0" onMouseClicked="#fourPlayers" prefHeight="45.0" prefWidth="48.0" style="-fx-background-repeat: no-repeat;" stylesheets="@proStyle.css" />
        <Label fx:id="backButton" layoutX="14.0" layoutY="14.0" onMouseClicked="#backAction" prefHeight="45.0" prefWidth="48.0" style="-fx-background-repeat: no-repeat;" stylesheets="@proStyle.css" />
