    <children>
        <ImageView pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@NewGame.jpg" />
            </image>
        </ImageView>
        <Label layoutX="274.0" layoutY="212.0" prefHeight="58.0" prefWidth="370.0" text="How many Players ?">
